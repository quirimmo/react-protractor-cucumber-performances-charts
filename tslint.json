{
	"extends": [
		"tslint:recommended",
		"tslint-react",
		"tslint-eslint-rules",
		"tslint-clean-code"
	],
	"rules": {
		"max-line-length": {
			"options": [120]
		},
		"new-parens": true,
		"no-arg": true,
		"no-bitwise": true,
		"no-conditional-assignment": true,
		"no-consecutive-blank-lines": false,
		"no-console": {
			"severity": "error",
			"options": ["debug", "info", "log", "time", "timeEnd", "trace"]
		}
	}
}
